{
  "name": "tecina",
  "description": "Phaser 3 with TypeScript using Vite.",
  "version": "0.0.1",
  "type": "module",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/aperezcastagnino/tecina"
  },
  "license": "MIT",
  "homepage": "https://github.com/aperezcastagnino/tecina#readme",
  "scripts": {
    "start": "vite",
    "build": "tsc && vite build",
    "lint": "eslint . --ext .js --ext .jsx --ext .ts --ext .tsx && tsc",
    "prettier:check": "prettier . --check",
    "prettier:write": "prettier . --write",
    "checks": "npm run prettier:write && npm run lint"
  },
  "lint-staged": {
    "**/*": "prettier --write --ignore-unknown"
  },
  "dependencies": {
    "phaser": "^3.80.1"
  },
  "devDependencies": {
    "@types/node": "^22.10.1",
    "eslint": "^8.57.1",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-airbnb-typescript": "^18.0.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-check-file": "^2.8.0",
    "eslint-plugin-deprecation": "^3.0.0",
    "lint-staged": "^15.2.10",
    "prettier": "3.3.3",
    "typescript": "^5.5.4",
    "typescript-eslint": "^7.18.0",
    "vite": "^5.4.2",
    "vite-tsconfig-paths": "^4.0.5"
  }
}
